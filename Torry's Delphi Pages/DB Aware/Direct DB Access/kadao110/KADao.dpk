{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $00400000}
{$DESCRIPTION 'KADao'}
{$DESIGNONLY}
{$IMPLICITBUILD ON}

requires
  vcl30,
  vclx30,
  vcldb30;

contains
  KDaoTable,
  KDaoDataBase,
  KADaoInfo,
  KADaoReg,
  KDaoDBEngine,
  KDaoWorkspace,
  KADAOEncrypter,
  KADaoConnectionCheck;

end.
